	<ctl name="RX1 MIX2 INP2" value="ZERO" />
	<ctl name="RX1 MIX2 INP1" value="ZERO" />
	<ctl name="RX7 MIX1 INP2" value="ZERO" />
	<ctl name="RX7 MIX1 INP1" value="ZERO" />
	<ctl name="RX6 MIX1 INP2" value="ZERO" />
	<ctl name="RX6 MIX1 INP1" value="ZERO" />
